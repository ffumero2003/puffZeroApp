{
  "expo": {
    "name": "PuffZero",
    "slug": "puffzero",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-puff-zero.png",
    "scheme": "puffzero",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-puff-zero.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F3FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.puffzero"
    },
    "android": {
      "package": "com.anonymous.puffzero",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "puffzero"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-puff-zero.png"
    },
    "extra": {
      "supabaseRedirectUrl": "puffzero://reset-password",
      "googleRedirectUrl": "puffzero://auth/callback"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
